{
    "manifest_version": 2,
    "name": "Factchecker",
    "version": "1.0",
    "description": "Automatic Factchecker on Video Elements",
    "icons": {
      "48": "48.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ],
    "browser_action": {
        "default_popup": "popup/ui.html"
    }
  }